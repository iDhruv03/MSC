// Question: Check if the entered number is prime or not

#include<iostream>
using namespace std;

int main(){

    int n;
    cin>>n;

    int a = 2;
    while (a<=2)
    {
        if (n % a == 0)
        {
            cout<<"Not Prime"<<endl;
            return 0;
        }
        a = a + 1;
    }
    cout<<"Prime Number";

    return 0;
}
