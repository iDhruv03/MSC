/*
Question : Print the following pattern for n rows:
1
22
333
4444
*/

#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n;
    int rows = 1;
    while (rows<=n)
    {
        int col = 1;
        while (col<=rows)
        {
            cout<<rows;
            col = col + 1;
        }
        cout<<endl;
        rows = rows + 1;
    }
    

    

    return 0;
}
