// Question: Read 3 numbers from stdin and print their sum to stdout.


#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int a,b,c;
    cin>> a >> b >> c;
    cout<<a+b+c; 
    return 0;
}
