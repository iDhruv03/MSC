// Question: Convert Fahrenheit to Celsius for a given temperature

#include<iostream>
using namespace std;

int main(){

    float F;
    cout<<"Pls enter the temp in Fahrenheit:"<<endl;
    cin>>F;
    float C = (5.0/9.0) * (F - 32);
    cout<<"The temp in Celsius is: "<<C<<endl;


    return 0;
}
